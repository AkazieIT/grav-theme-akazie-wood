<style>

::-webkit-scrollbar {
  width: 3px;
}

         ::-webkit-scrollbar-track {
          background: {{theme_var('style.color.background.main')}};
        }
       ::-webkit-scrollbar-thumb {
          background: {{theme_var('style.color.background.secondary')}};
        }
       ::-webkit-scrollbar-thumb:hover {
          background: {{theme_var('style.color.text.nav.default')}};
        }


  html, body{
    font-family: {{ theme_var('style.fonts.main') | raw }};
    color: {{ theme_var('style.color.text.main') }};
  }
  body {
    background-color: {{ theme_var('style.color.background.main') }};
  }
  .wrapper{
    background-color: {{ theme_var('style.color.background.main') }};
  }




        header.site-header div.header-inner nav.header-nav.mobile::-webkit-scrollbar-track {
          background: {{theme_var('style.color.background.main')}};
        }
        header.site-header div.header-inner nav.header-nav.mobile::-webkit-scrollbar-thumb {
          background: {{theme_var('style.color.background.secondary')}};
        }
        header.site-header div.header-inner nav.header-nav.mobile::-webkit-scrollbar-thumb:hover {
          background: {{theme_var('style.color.text.nav.default')}};
        }


  h1, h2, h3, h4, h5, h6{
    font-family: {{ theme_var('style.fonts.h') | raw }};
  }
  {% if theme_var('site.parallaxheader') %}
  header.site-header{
    background-color: rgba(255,255,255,0);
    background-image: url('{{ page.header.image  ? page.media[page.header.image].url : theme_url ~ '/images/home.jpg' }}');
    background-attachment: fixed;
    background-position: top;
    background-repeat: no-repeat;
    background-size: cover;
  }
  {% else %}

  header.site-header{
    background-color: {{ theme_var('style.color.background.nav.default') }};
  }
  header.site-header div.header-inner nav.header-nav.desktop ul li a{
    background-color: {{ theme_var('style.color.background.nav.default') }};
  }
  {% endif %}

  header.site-header div.header-inner nav.header-nav.desktop ul li a{
    color: {{ theme_var('style.color.text.nav.default') }};
    font-family: {{ theme_var('style.fonts.nav') | raw }};
  }
  header.site-header div.header-inner nav.header-nav.desktop ul li a.active,
  header.site-header div.header-inner nav.header-nav.desktop ul li a:hover{
    background-color: {{ theme_var('style.color.background.nav.hover') }};
    color: {{ theme_var('style.color.text.nav.hover') }};
  }

  header.site-header div.header-inner nav.header-nav.desktop ul li.specialbutton a,
    header.site-header div.header-inner nav.header-nav.mobile ul li.specialbutton a{

    background-color:{{ theme_var('style.color.background.secondary') }};
    color:{{ theme_var('style.color.text.secondary') }};
  }
  header.site-header div.header-inner nav.header-nav.desktop ul li.specialbutton a.active,
  header.site-header div.header-inner nav.header-nav.desktop ul li.specialbutton a:hover,
  header.site-header div.header-inner nav.header-nav.mobile ul li.specialbutton a.active,
  header.site-header div.header-inner nav.header-nav.mobile ul li.specialbutton a:hover{

    color:{{ theme_var('style.color.background.secondary') }};
    background-color:{{ theme_var('style.color.text.secondary') }};
  }


.testi-cont {
  color:{{ theme_var('style.color.text.secondary') }};

    background-color: {{ theme_var('style.color.background.primary') }};

    background: {{ theme_var('style.color.background.primary') }};
    background: linear-gradient(0deg, {{ theme_var('style.color.background.primary') }} 85%, rgba(255,255,255,0) 85%);

}


  footer.site-footer .footer-main{
    font-family: {{ theme_var('style.fonts.footer') | raw }};
  }
  .markdown ol li::before, .markdown ul:not(.sociallinks) li::before{
    background-color: {{ theme_var('style.color.background.primary') }};
  }
  .markdown blockquote{
    background-color: {{ theme_var('style.color.background.secondary') }};
    border: 1px solid {{ theme_var('style.color.background.secondary') }};
    color: {{ theme_var('style.color.text.secondary') }};
  }
  .markdown code{
    color: {{ theme_var('style.color.text.cta') }};
  }
  a{
    color: {{ theme_var('style.color.text.cta') }};
  }
  a:hover{
    color: {{ theme_var('style.color.text.cta') }};
  }
  .screen-image .image-caption{
    color: {{ theme_var('style.color.white') }};
    text-shadow: 1px 1px 2px {{ theme_var('style.color.black') }};
  }
  main.site-content .modular.section-image .caption_1,
  main.site-content .modular.section-map .caption_1{
    background-color: {{ theme_var('style.color.black') }};
    color: {{ theme_var('style.color.white') }};
  }
  main.site-content .modular.section-gallery .gallery>article{
    border: 14px solid {{ theme_var('style.color.white') }};
  }
  {% if theme_var('site.parallax') %}
  main.site-content { background-color: {{ theme_var('style.color.white') }}; }
  header.site-header{ box-shadow: none; }
  {% endif %}

  footer.site-footer{
    {% if theme_var('footer.fullscreen.enabled') %}
    background-color: {{ theme_var('footer.fullscreen.bgcolor') }};
    {% else %}
    background-color: {{ theme_var('style.color.background.secondary') }};
    {% endif %}
    color: {{ theme_var('style.color.text.secondary') }};
  }
  footer.site-footer .footer-top{
    background-color: {{ theme_var('style.color.background.primary') }};
  }
  footer.site-footer .footer-main .footer-navigation ul li a{
    color: {{ theme_var('style.color.text.secondary') }};
  }
  footer.site-footer .footer-main .footer-adress,
  footer.site-footer .footer-main .footer-email,
  footer.site-footer .footer-main .footer-email a{
    color: {{ theme_var('style.color.text.secondary') }};
  }
  div.mobile-toggle .pull span{
    background-color: {{ theme_var('style.color.background.nav.hover') }};
  }
  header.site-header div.header-inner nav.header-nav.mobile{
    background-color: {{ theme_var('style.color.background.nav.default') }};
  }
  {% if theme_var('site.parallaxheader') %}
  header.site-header div.header-inner nav.header-nav.mobile ul li a{
    color: {{ theme_var('style.color.black') }};
    font-family: {{ theme_var('style.fonts.nav') | raw }};
  }
  {% else %}
  header.site-header div.header-inner nav.header-nav.mobile ul li a{
    color: {{ theme_var('style.color.text.nav.default') }};
    font-family: {{ theme_var('style.fonts.nav') | raw }};
  }
  {% endif %}


  header.site-header div.header-inner nav.header-nav.mobile ul li a.active{
    color: {{ theme_var('style.color.background.nav.hover') }};
  }
  a.linksbutton, a.teamemail {
    background-color:{{ theme_var('style.color.background.primary') }};
    color:{{ theme_var('style.color.text.primary') }};
    border:1px solid {{ theme_var('style.color.background.primary') }};
  }
  a.linksbutton:hover, a.teamemail:hover {
    background-color: {{ theme_var('style.color.background.secondary') }};
    color:{{ theme_var('style.color.text.secondary') }};
    border: 1px solid {{ theme_var('style.color.text.secondary') }};
  }
/*
  header.site-header { height: $nav-height; }
  header.site-header  div.header-inner img {
     margin-top: $nav-height / 20;
     height: $nav-height - ($nav-height / 10); }
  header.site-header  div.header-inner ul li a { line-height: $nav-height; }
  .image-caption{ padding-top: $nav-height; }
  */
  header.site-header  div.header-inner ul li a { line-height: 60px !important; }
  header.site-header  div.header-inner ul li.specialbutton a {    line-height: 60px !important;  }
  header.site-header  div.header-inner ul li.specialbutton { margin-top: 0px; }
  {% if theme_var('nav.navheight') >= 20 and theme_var('nav.navheight') < 206 %}
  @media (min-width:992px) {
  header.site-header { height: {{ theme_var('nav.navheight') }}px !important; }
  header.site-header  div.header-inner img {
     margin-top: {{ theme_var('nav.navheight') / 20 }}px !important;
     height: {{ theme_var('nav.navheight') - (theme_var('nav.navheight') / 10) }}px !important; }
  header.site-header  div.header-inner ul li a { line-height: {{ theme_var('nav.navheight') }}px !important; }
  header.site-header  div.header-inner ul li.specialbutton a {    line-height: 60px !important;  }
  header.site-header  div.header-inner ul li.specialbutton { margin-top: {{ (theme_var('nav.navheight') / 2) - 30 }}px; }
  .image-caption{ padding-top: {{ theme_var('nav.navheight') }}px !important; }
  .image-caption.claim .claim, .image-caption.claim .h3 {
    margin-top:{{ 190 - theme_var('nav.navheight') }}px !important;
  }
  }

  .screen-image.screen-top.removeheader { height: {{ theme_var('nav.navheight') }}px; }
  {% endif %}
  {% if( theme_var('nav.mobileheightenabled') ) %}
  @media (max-width:992px) {
    header.site-header { height:{{ theme_var('nav.mobilenavheight') }}px;  }
    header.site-header div.header-inner div.logo img { height:{{ theme_var('nav.mobilenavheight') - 10 }}px; }
  }
  {% endif %}
  footer.site-footer .footer-main .container.footerpage .upfootertext a {
    color:{{ theme_var('style.color.white') }};
  }

  body .wrapper .breaktitle .breakwrap h1 {
    color:{{ theme_var('style.color.text.nav.default') }};
  }

  body ul.catfilter li a.button,  body ul.catfilter li a.button.filterreset {
    background-color: transparent;
    color:{{ theme_var('style.color.text.secondary') }};
  }
  body ul.catfilter li a.button:hover,   body ul.catfilter li a.button.filterreset:hover {
    color:{{ theme_var('style.color.white') }};
  }



    body ul.catfilter li a.button.keyyes {
      background-color:transparent;
      color:{{ theme_var('style.color.text.secondary') }};
      border: 1px solid transparent;
    }
    body ul.catfilter li a.button.keyyes:hover {
      border: 1px solid {{ theme_var('style.color.white') }};
    }

    body ul.catfilter li a.button.is-active, body ul.catfilter li a.button.keyyes.is-active   {
      background-color:{{ theme_var('style.color.white') }};
      color:{{ theme_var('style.color.text.secondary') }};
    }

    body ul.catfilter li a.button.is-active:hover, body ul.catfilter li a.button.keyyes.is-active:hover {
      color:{{ theme_var('style.color.text.secondary') }};

    }
    body .catfiltercontainer { background-color:{{ theme_var('style.color.background.secondary') }}; }


  {% if theme_var('site.maxwidth') != 1280 and theme_var('site.maxwidth') >= 999 and theme_var('site.maxwidth') < 2001  %}
    @media (min-width: 1220px) {
      .image-caption.claim h3 { max-width: {{ theme_var('site.maxwidth') }}px; }
      header.site-header.fittowidth .header-inner { max-width: {{ theme_var('site.maxwidth') }}px; }
      .breaktitle .breakwrap { max-width: {{ theme_var('site.maxwidth') }}px; }
      main.site-content .page.section-default, main.site-content .page.section-modular  { max-width: {{ theme_var('site.maxwidth') }}px; }
    }
  {% endif %}

  div.stickybottom {
    background-color: {{ theme_var('style.color.background.nav.default') }};
    color: {{ theme_var('style.color.text.primary') }};
  }
  .bookinghover { background-color: {{ theme_var('style.color.background.secondary') }};  }
  a.linksbutton.bookinghovermobile { color:{{ theme_var('style.color.text.secondary') }} !important;background-color: {{ theme_var('style.color.background.secondary') }}; border:none; }
  .bookinghover ul li a, .bookinghover ul li {  color:{{ theme_var('style.color.text.secondary') }};  }
  a.linksbutton.bookinghovermobile:hover { background-color: {{ theme_var('style.color.text.secondary') }} !important; color: {{ theme_var('style.color.background.secondary') }} !important; }
  .bookinghover ul li a:hover {  color:{{ theme_var('style.color.background.main') }};  }

  .list-item.h-entry.oz-filteritem { background-color: #d6d6d6; }
  .oz-filteritem a.filterbutton {
    color: {{ theme_var('style.color.white') }};
    background-color: {{ theme_var('style.color.text.primary') }};
  }
  .oz-filteritem a.filterbutton:hover {
    color: {{ theme_var('style.color.text.primary') }};
    background-color: {{ theme_var('style.color.white') }};
  }


</style>
